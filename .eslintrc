{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "airbnb-base",
        "plugin:vue/vue3-essential"
    ],
    "parser": "vue-eslint-parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "vue/html-indent": ["error", 2],
        "indent": ["error", 2],
        "no-alert": 0,
        "no-unused-vars": "warn",
        "spaced-comment": "off",
        "no-param-reassign": 0,
        "import/extensions": 0,
        "import/no-unresolved": 0,
        "import/prefer-default-export": 0,
        "import/no-extraneous-dependencies": ["error", {
            "devDependencies": true
        }]
    }
}
